            var mapOptions = {center: myCenter, zoom: 10};
            var map = new google.maps.Map(mapCanvas, mapOptions);
            var marker = new google.maps.Marker({position:belgrano});
            marker.setMap(map);
            var marker2 = new google.maps.Marker({position:urquiza});
            marker2.setMap(map);
            var marker3 = new google.maps.Marker({position:floresta});
            marker3.setMap(map);
            var marker4 = new google.maps.Marker({position:bernal});
            marker4.setMap(map);
            var marker5 = new google.maps.Marker({position:saavedra});
            marker5.setMap(map);




        }
    </script>

